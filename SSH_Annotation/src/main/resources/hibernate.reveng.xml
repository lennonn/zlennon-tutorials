<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering PUBLIC "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-reverse-engineering-3.0.dtd" >

<hibernate-reverse-engineering>

	<table name="CATERGORIES" schema="SCOTT"
		class="com.zl.dto.domain.Category">
		<primary-key>
			<generator class="sequence"></generator>
		</primary-key>
	</table>
	<table name="FILES" schema="SCOTT"
		class="com.zl.dto.domain.FileSource">
		<primary-key>
			<generator class="sequence"></generator>

		</primary-key>
	</table>
	<table name="CHILD_CATEGORIES" schema="SCOTT"
		class="com.zl.dto.domain.ChildCategory">
		<primary-key>
			<generator class="sequence"></generator>
		</primary-key>
	</table>
	<table name="CATEGORIES" schema="SCOTT"></table>
	<table name="ROOT_CATEGORY" schema="SCOTT"
		class="com.zl.dto.domain.Category">
		<primary-key>
			<generator class="sequence"></generator>
		</primary-key>
	</table>
<table name="T_USER" schema="SCOTT" class="com.zl.dto.domain.User">
	<primary-key>
		<generator class="uuid.string"></generator>
	</primary-key>
</table>
</hibernate-reverse-engineering>